  <script defer src="https://use.fontawesome.com/releases/v5.0.7/js/all.js"></script>
</head>

<body>
  <section id="title" class="nav_prob">
    <div class="container-profile">
      <nav class="navbar navbar-expand-lg navbar-dark" class = "Navabar">
        <a class="navbar-brand" href=""> Trackk It</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon">
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">



          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="/profile">Profile</a>
            </li>

            <li class="nav-item">
              <a class="nav-link" href="/food">Add food</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/exercise">Exercise</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/track">Track</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/logout">LogOut</a>
            </li>


          </ul>
        </div>
      </nav>
</section>




<div class="row no-gutters">
  <div class="col-md-6 no-gutters">
    <div class="leftside  r">

<div class="centered">


    <h2 style = "color : black"> Today's data</h2>
    <table class="table table-striped table-dark">
  <thead>
    <tr>


      <th style = "width : 20%" scope="col">Nutrition</th>
      <th   style = "width : 20%" scope="col">Value</th>

    </tr>
  </thead>
  <tbody>
    <tr>

      <td>Calorie</td>
      <td><%=cal%></td>

    </tr>
    <tr>

      <td>Protein</td>
      <td><%=pr%>g</td>

    </tr>
    <tr>

      <td>Cholesterol</td>
      <td><%=ch%>mg</td>

    </tr>
    <tr>

      <td>Fat</td>
      <td><%=fa%>g</td>

    </tr>
    <tr>
      <td>Carbohydrates</td>
      <td><%=car%>g</td>

    </tr>

  </tbody>
  </table>
</div>
  </div>
 </div>

<div class="col-md-6 no-gutters">
  <div class="rightside ">
    <div class="centered">
    <h2 style = "color : black"> Welcome, <%=n%></h2>
    <table class="table table-striped table-dark  ">
  <thead>
    <tr>


      <th style = "width : 20%" scope="col">Information</th>
      <th   style = "width : 20%" scope="col">Value</th>

    </tr>
  </thead>
  <tbody>

    <tr>

      <td>Height</td>
      <td> <%=h%>cm</td>

    </tr>
    <tr>

      <td>Weight</td>
      <td> <%=w%>kg</td>

    </tr>
    <tr>

      <td>BMI</td>
      <td><%=b%></td>

    </tr>
    <tr>

      <td>BMR</td>
      <td><%=bm%></td>

    </tr>
    <tr>
      <td>Ideal Calorie intake</td>
      <td><%=c%></td>

    </tr>

  </tbody>
</table>
</div>
  </div>
</div>
</div>

</body>
</html>
